{
  "name": "react-quickstart",
  "version": "1.0.0",
  "description": "React starter kit for single page web applications.",
  "scripts": {
    "preremove-demo": "npm run remove-dist",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel  open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"./{,!(node_modules)/**/}*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js && npm run open:dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blurbyte/react-quickstart"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "css-modules",
    "quickstart"
  ],
  "author": "blurbyte",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "redux": "3.5.2",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "1.0.0",
    "es6-promise": "3.2.1"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "css-loader": "0.24.0",
    "eslint": "3.3.1",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.2",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "mocha": "3.0.2",
    "npm-run-all": "3.0.0",
    "open": "0.0.5",
    "postcss-loader": "0.10.1",
    "redux-immutable-state-invariant": "1.2.3",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-md5-hash": "0.0.5"
  }
}
